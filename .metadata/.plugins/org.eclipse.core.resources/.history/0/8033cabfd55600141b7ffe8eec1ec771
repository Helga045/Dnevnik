import java.util.Random;


public class gostudy {

	public static void main(String[] args) {
		
		String[] work = {"Займись математикой","Займись физикой","Займись русский","Займись английским",
				"Иди почитай","Иди почитай книгу на английском","Иди учи слова","Иди делай ДЗ","Займись программированием",
				"Займис сценаристикой(т)","Займис сценаристикой(пр)", "Займись латынью","Иди покрути обруч",
				"Иди качай пресс","Порисуй","Отдохни","Поиграй на гитаре"};
		int [] time = {10, 15, 30, 60, 90};
		
		int rW = (int) (Math.random()*(work.length-1));
		int rT = (int) (Math.random()*(time.length-1));
		
		System.out.println(work[rW] +", "+time[rT]+" минут!!!");
	}
}
